<!DOCTYPE html>
<html lang="en">
    <%-  include('./partials/head') %>
<body>
    <%-  include('./partials/nav') %>
    <div>
       <% if(blogs.length>0){ %>
        <% blogs.forEach((blog)=>{ %>
        <a href="/blog/<%= blog._id %>"> <h3><%= blog.title %></h3>
            <p><%= blog.snippet %></p></a>
       <% }) %>
       <% } else{ %>
      <p> there are no blogs to display</p>
    <% }  %>
    </div>
    <%-  include('./partials/footer') %>
</body>
</html>